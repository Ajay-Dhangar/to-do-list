{
    "manifest_version": 3,
    "name": "ToDoList",
    "version": "1.0",
    "description": "This extension will help you to manage your tasks",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "128": "logo.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["index.html"],
        "matches": ["<all_urls>"]
      }
    ],
    "author": "Ajay-Dhangar"
  }
  
